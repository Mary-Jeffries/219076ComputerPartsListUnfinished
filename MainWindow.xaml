    Feb 14-21 2020
    Program that displays parts of a computer and the prices
    -->
    <Window x:Class="_219076ComputerParts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_219076ComputerParts"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="30" Background="#FFCCD4FF">
    <StackPanel>
        <Button x:Name="btnParts" Content="Computer Parts List" Click="btnParts_Click" Background="White" BorderBrush="Black" HorizontalAlignment="Left" Width="280" />
        <Button x:Name="btnPrice" Content="Price of Parts" Click="btnPrice_Click" Background="White" BorderBrush="Black" HorizontalAlignment="Left" Width="280" />    
    </StackPanel>
</Window>
